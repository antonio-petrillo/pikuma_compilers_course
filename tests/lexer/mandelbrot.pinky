func mandelbrot(cx, cy)
    x := 0
    y := 0
    iter := 0
    max := 16
    while x*x + y*y <= 4 and iter < max do
        xtemp := x*x - y*y + cx
        y := 2 * x * y + cy
        x := xtemp
        iter := iter + 1
    end
    ret iter
end

